{
  "name": "study-in-germany",
  "version": "1.0.0",
  "description": "Your gateway to German higher education",
  "private": true,
  "scripts": {
    "dev": "bash -c 'if [ ! -d node_modules ]; then echo \"Installing depsâ€¦\" && npm install; fi && next dev --port 9002 --hostname 0.0.0.0'",
    "build": "next build",
    "build:production": "next build && next export",
    "start": "next start",
    "serve:static": "npx serve out -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "node scripts/bundle-analyzer.js",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "clean": "rm -rf .next out",
    "postinstall": "npm run type-check",
    "export": "next build && next export",
    "preview": "npx serve -s out -l ${PORT:-9002}"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "clsx": "^2.0.0",
    "next": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.0",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "study-germany",
    "german-universities",
    "international-students",
    "education",
    "scholarships",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "author": {
    "name": "Study in Germany Team",
    "email": "info@study-in-germany.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/study-in-germany.git"
  }
}
